<body>
    <main id="login">
        <div class="log">
        
            
            <h1>Inicia sesión </h1>
            
            <form action="/login" method="post" id="logs">
                
                
                <input type="email" name= "correo" id="correo" placeholder="Correo electronico" autocomplete="on">
                <label for="correo"></label>
                <input type="password" name="contraseña" id="contra" placeholder="Contraseña" required autocomplete>
                <label for="contra"></label>
                   
                    <label for="recordar"> 
                    <input type="checkbox" name="recordar" id="recordar" value="recordar">Recordar
                    </label>
                   
                <button type="submit">Entrar</button>
            </form>
            </div>
        
    </main>
